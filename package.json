{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,scss,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,scss,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier-eslint --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.4",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.26"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.26",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "prettier-eslint-cli": "^6.0.1",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
